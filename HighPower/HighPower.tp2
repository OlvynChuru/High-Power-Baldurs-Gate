	COPY_EXISTING ~anisnd.ids~ ~override~
		FOR (i = start_anisnd; i < 0xFFFF; ++i) BEGIN
			READ_2DA_ENTRY i 0 3 current
			PATCH_IF (current > slotDecimal) BEGIN
